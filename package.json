    "lint-staged": "^10.5.4",
    "prettier": "~1.18.2",
    "supertest": "~4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "~6.2.1",
    "ts-node": "~8.6.0",
    "tsconfig-paths": "~3.9.0",
    "typescript": "~3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
